YenButton.ZIndex = 5
YenButton.Text = ""
YenButton.TextTransparency = 1.000
YenButton.MouseButton1Click:Connect(function()
	if ToggleYen == true then
		On:TweenPosition(
			UDim2.new(0.53, 0, 0.083, 0),
			"Out",
			"Quad",
			0.5,
			false
		)
		wait(0.5)
		Multiplier = 100
		ToggleFarmYen = true
		ToggleYen = false
		while ToggleFarmYen == true do task.wait(0)
			for i = 10, Multiplier do
				task.spawn(function()
					Replicated.RSPackage.Events.GeneralFunction:InvokeServer("LotteryRandomDraw")
					Replicated.RSPackage.Events.GeneralEvent:FireServer("SpinFinished")
				end)
			end
		end
	else
		if ToggleYen == false then
			On:TweenPosition(
				UDim2.new(0.048, 0, 0.083, 0),
				"Out",
				"Quad",
				0.5,
				false
			)
			wait(0.5)
			Multiplier = 0
			ToggleYen = true
		end
	end
end)

