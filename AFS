EspFruitButton.Parent = EspUI
EspFruitButton.Active = false
EspFruitButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
EspFruitButton.BorderColor3 = Color3.fromRGB(27, 42, 53)
EspFruitButton.Position = UDim2.new(0.72167927, 0, 0.18280156, 0)
EspFruitButton.Selectable = false
EspFruitButton.Size = UDim2.new(0.236306846, 0, 0.63418752, 0)
EspFruitButton.ZIndex = 5
EspFruitButton.Text = ""
EspFruitButton.TextTransparency = 1.000
EspFruitButton.MouseButton1Click:Connect(function()
	if ToggleEsp == true then
		On_3:TweenPosition(
			UDim2.new(0.53, 0, 0.083, 0),
			"Out",
			"Quad",
			0.5,
			false
		)
		wait(0.5)
		ToggleEspFruit = true
		ToggleEsp = false
		while ToggleEspFruit == true do
			for i,v in pairs(game.Workspace.MiscModels:GetDescendants()) do
				if v:IsA("Part") and v.Name == "ClickBox" and v.Parent.Name == "Model"  then
					EspFruitUI.Parent = v.Parent
					EspFruitUI.AlwaysOnTop = true
					EspFruitUI.LightInfluence = 1
					EspFruitUI.Size = UDim2.new(0, 50, 0, 50)
					EspFruitUI.StudsOffset = Vector3.new(0, 2, 0)

					FruitEspText.Parent = EspFruitUI
					FruitEspText.BackgroundColor3 = Color3.new(1, 1, 1)
					FruitEspText.BackgroundTransparency = 1
					FruitEspText.Size = UDim2.new(1, 0, 1, 0)
					FruitEspText.Text = "Devil Fruit"
					FruitEspText.TextColor3 = Color3.new(1, 1, 0)
					FruitEspText.TextScaled = true
					FruitEspText.Font = "Fantasy"
				end
			end
		end
	else
		if ToggleEsp == false then
			On_3:TweenPosition(
				UDim2.new(0.048, 0, 0.083, 0),
				"Out",
				"Quad",
				0.5,
				false
			)
			wait(0.5)
			EspFruitUI:Destroy()
			ToggleEsp = true
		end
	end
end)

